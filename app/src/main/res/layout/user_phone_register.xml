<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"

    >

    <View
        android:layout_marginRight="55dp"
        android:layout_marginLeft="55dp"
        android:background="#dbdddd"
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true"
        android:id="@+id/view" />
    <View
        android:layout_marginRight="55dp"
        android:layout_marginLeft="55dp"
        android:background="#dbdddd"
        android:layout_width="match_parent"
        android:layout_height="0.7dp"
        android:layout_marginTop="5dp"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true"
        android:id="@+id/view1" />
    <!--这是手机图片-->


    <TextView
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:text="设置密码"
        android:textColor="#4a5058"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="40dp"
        android:id="@+id/textView2"
        android:layout_centerHorizontal="true" />

    <!--这是下一步按钮-->
    <Button
        android:layout_marginTop="90dp"
        android:background="#00ffffff"
        android:text="下一步"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#c1c0be"
        android:layout_below="@+id/view"
        android:layout_centerHorizontal="true"
        android:id="@+id/user_btn_next_register" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout"
        android:layout_below="@+id/textView2"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="85dp">
        <!--中国号码区号-->
        <TextView
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="输入密码："
            android:layout_width="75dp"
            android:layout_height="30dp"
            android:id="@+id/textView"/>
        <!--这是手机输入editText-->
        <EditText
            android:layout_gravity="center"
            android:textColorHint="#c8c9c9"
            android:maxLength="16"
            android:imeOptions="actionDone"
            android:layout_marginLeft="10dp"
            android:id="@+id/user_edit_phone_register"
            android:layout_marginTop="5dp"
            android:background="#00ffffff"
            android:gravity="left"
            android:inputType="textPassword"
            android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
            android:maxLines="1"
            android:saveEnabled="true"
            android:layout_width="160dp"
            android:textSize="15sp"
            android:layout_height="25dp" />
        <ImageView
            android:visibility="invisible"
            android:layout_marginTop="12dp"
            android:id="@+id/showPassword_one"
            android:src="@drawable/ic_eyeon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <!--这是注册按钮-->
    <Button
        android:id="@+id/user_btn_continue"
        android:textColor="#e7bc11"
        android:text="返回"
        android:background="#00ffffff"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        />

    <LinearLayout

        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/linearLayout1"
        android:layout_marginTop="21dp"
        android:layout_below="@+id/view"
        android:layout_alignStart="@+id/linearLayout">
        <!--中国号码区号-->
        <TextView
            android:layout_marginTop="10dp"
            android:gravity="center"
            android:text="确认密码："
            android:layout_width="75dp"
            android:layout_height="30dp"
            android:id="@+id/textView1"/>
        <!--这是手机输入editText-->
        <EditText
            android:layout_marginTop="5dp"
            android:layout_gravity="center"
            android:textColorHint="#c8c9c9"
            android:maxLength="16"
            android:imeOptions="actionDone"
            android:layout_marginLeft="10dp"
            android:id="@+id/user_edit_phone_register_sure"
            android:background="#00ffffff"
            android:gravity="left"
            android:inputType="textPassword"
            android:digits="0123456789abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
            android:maxLines="1"
            android:saveEnabled="true"
            android:layout_width="160dp"
            android:textSize="15sp"
            android:layout_height="25dp" />

        <ImageView
            android:visibility="invisible"

            android:layout_marginTop="12dp"
            android:id="@+id/showPassword_two"
            android:src="@drawable/ic_eyeon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
</RelativeLayout>
